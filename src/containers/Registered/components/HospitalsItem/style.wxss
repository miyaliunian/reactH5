.hospitalsItem_wrapper {
  padding-top: 40px;
}
.hospitalsItem__con {
  padding: 20px;
  overflow: hidden;
  background-color: white;
}
.hospitalsItem__con .hospitalsItem__title {
  font-size: 15px;
}
.hospitalsItem__con .hospitalsItem__middle {
  margin: 12px 0;
  width: 100%;
  display: flex;
  flex-direction: row;
}
.hospitalsItem__con .hospitalsItem__middle .hospitalsItem__middle__item {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.hospitalsItem__con .hospitalsItem__middle .hospitalsItem__middle__item .hospitalsItem__middle__icon {
  width: 12px;
  height: 12px;
  margin-right: 3px;
  overflow: hidden;
}
.hospitalsItem__con .hospitalsItem__middle .hospitalsItem__middle__item .hospitalsItem__middle__innerTxt {
  margin-right: 10px;
  font-size: 13px;
  color: cornflowerblue;
}
.hospitalsItem__con .hospitalsItem__address {
  font-size: 13px;
  color: #666666;
}
